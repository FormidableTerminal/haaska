#define S_magenta       #d33682
#define S_violet        #6c71c4
#define S_blue          #268bd2
#define S_cyan          #2aa198
#define S_green         #859900

#define  COLOR_BG             #121212
#define  COLOR_FG             #d0d0d0
#define  COLOR_RED_DARK       #dc322f
#define  COLOR_RED_LIGHT      #cb4b16
#define  COLOR_GREEN_DARK     #d7ffaf
#define  COLOR_GREEN_LIGHT    #a6e22e
